{
	"openapi": "3.0.0",
	"info": {
		"title": "API de Exemplo",
		"description": "API para demonstração",
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "http://localhost:3000",
			"description": "Servidor de Desenvolvimento"
		}
	],
	"paths": {
		"/users": {
			"get": {
				"summary": "Obter todos os usuários",
				"responses": {
					"200": {
						"description": "Lista de usuários",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/User"
									}
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"User": {
				"type": "object",
				"properties": {
					"id": {
						"type": "integer"
					},
					"name": {
						"type": "string"
					},
					"email": {
						"type": "string"
					}
				},
				"required": [
					"id",
					"name",
					"email"
				]
			}
		}
	}
}