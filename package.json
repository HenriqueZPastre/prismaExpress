		"@types/express": "^4.17.17",
		"@types/node": "^18.16.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/xml2js": "^0.4.11",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
