		"zod": "^3.21.4"
	},
	"scripts": {	
		"runBuild": "ts-node -r tsconfig-paths/register ./dist/app.js",
		"build": "tsc",
		"swagger": "ts-node ./src/swagger/main.ts",
		"cz": "cz",
		"lint:fix": "eslint . --ext .ts --fix",
		"lint": "eslint . --ext .ts",
		"prisma": "npx prisma studio",
		"dev": "tsnd --respawn ./app.ts",
		"migrate": "npx prisma migrate dev --name init",
		"o": "npx cypress open",
		"r": "npx cypress run",
		"report": "npx mochawesome-merge 'cypress/results/*.json' > joinmochawesome.json && npx marge joinmochawesome.json && rimraf cypress/results "
	},
	"description": "Api REST com prisma, express, zod e testes.",
