{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
	"moduleResolution": "Node16",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "resolveJsonModule": true
  },
  "include": ["./**/*.ts", "./**/*.*.ts"]
}
